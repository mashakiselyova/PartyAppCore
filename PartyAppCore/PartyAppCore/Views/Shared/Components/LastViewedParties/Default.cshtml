@model List<Party>

<table border="1">
    <thead>
        <tr>
            <th>Последние просмотры:</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var party in Model)
        {
            <tr>
                <td>@Html.ActionLink(party.Name, "ShowParty", "Party", new { id = party.Id }, null)</td>
            </tr>

        }
    </tbody>
</table>
